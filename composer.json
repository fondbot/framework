{
  "name": "fondbot/fondbot",
  "description": "Laravel framework library for creating Bots.",
  "type": "library",
  "keywords": [
    "fondbot",
    "bot",
    "bots",
    "chatbots",
    "laravel",
    "telegram",
    "facebook messenger",
    "vk"
  ],
  "license": "MIT",
  "homepage": "https://fondbot.com",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "vladimir@fondbot.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.2",
    "laravel/framework": "^5.4",
    "tightenco/collect": "^5.4"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.6",
    "laravel/laravel": "^5.4",
    "mockery/mockery": "^0.9.9",
    "phpunit/phpunit": "^5"
  },
  "autoload": {
    "psr-4": {
      "FondBot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure --no-coverage",
    "cs": "php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
